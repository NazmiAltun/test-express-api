{
  "name": "test-express-api",
  "version": "1.0.0",
  "description": "Test API",
  "main": "index.js",
  "author": "Nazmi Altun <nazmialtun@windowslive.com>",
  "license": "MIT",
  "keywords": [
    "test",
    "express",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:NazmiAltun/test-express-api.git"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "format": "prettier -c --write .",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests --coverage",
    "test:watch": "jest --watchAll --passWithNoTests",
    "prepare": "husky install",
    "pre-commit": "lint-staged --concurrent=false"
  },
  "lint-staged": {
    "*": "yarn format",
    "*.ts": "yarn lint"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-actuator": "^1.8.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/express-actuator": "^1.8.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-status-codes": "^2.2.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^14.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  }
}
